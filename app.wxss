.placeholder {
	color: #fff;
}

.dialog {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.5);
	z-index: 10;
}
.empty{
  text-align: center;
  margin-top: 200px;
  color: #666;
}
page {
	background: #f9fafd;
}

.hover:hover {
	opacity: 0.6;
}

.flex {
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.icon_right {
	width: 7px;
	height: 13px;
}

.down {
	width: 12px;
	height: 12px;
}

.relative {
	position: relative;
}
.row1{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: normal;
}
